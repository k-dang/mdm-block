{
  "manifest_version": 3,
  "name": "Master Duel Meta Ad Blocker",
  "version": "1.0",
  "description": "Blocks adds on the https://www.masterduelmeta.com/ website",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://www.masterduelmeta.com/*"
      ]
    }
  ]
}